#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/stevenvos/Documents/Projecten/builder/node_modules/.pnpm/vinxi@0.5.3_db0@0.3.1_ioredis@5.6.0_jiti@2.4.2_terser@5.39.0_tsx@4.19.3_typescript@5.8.2/node_modules/vinxi/bin/node_modules:/Users/stevenvos/Documents/Projecten/builder/node_modules/.pnpm/vinxi@0.5.3_db0@0.3.1_ioredis@5.6.0_jiti@2.4.2_terser@5.39.0_tsx@4.19.3_typescript@5.8.2/node_modules/vinxi/node_modules:/Users/stevenvos/Documents/Projecten/builder/node_modules/.pnpm/vinxi@0.5.3_db0@0.3.1_ioredis@5.6.0_jiti@2.4.2_terser@5.39.0_tsx@4.19.3_typescript@5.8.2/node_modules:/Users/stevenvos/Documents/Projecten/builder/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/stevenvos/Documents/Projecten/builder/node_modules/.pnpm/vinxi@0.5.3_db0@0.3.1_ioredis@5.6.0_jiti@2.4.2_terser@5.39.0_tsx@4.19.3_typescript@5.8.2/node_modules/vinxi/bin/node_modules:/Users/stevenvos/Documents/Projecten/builder/node_modules/.pnpm/vinxi@0.5.3_db0@0.3.1_ioredis@5.6.0_jiti@2.4.2_terser@5.39.0_tsx@4.19.3_typescript@5.8.2/node_modules/vinxi/node_modules:/Users/stevenvos/Documents/Projecten/builder/node_modules/.pnpm/vinxi@0.5.3_db0@0.3.1_ioredis@5.6.0_jiti@2.4.2_terser@5.39.0_tsx@4.19.3_typescript@5.8.2/node_modules:/Users/stevenvos/Documents/Projecten/builder/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../vinxi@0.5.3_db0@0.3.1_ioredis@5.6.0_jiti@2.4.2_terser@5.39.0_tsx@4.19.3_typescript@5.8.2/node_modules/vinxi/bin/cli.mjs" "$@"
else
  exec node  "$basedir/../../../../../../vinxi@0.5.3_db0@0.3.1_ioredis@5.6.0_jiti@2.4.2_terser@5.39.0_tsx@4.19.3_typescript@5.8.2/node_modules/vinxi/bin/cli.mjs" "$@"
fi
